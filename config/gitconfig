[user]
	name = Jason Liu
	email = my@email.com
[color]
	ui = true
[core]
	excludesfile = /Users/jason/.gitignore_global
[push]
	default = simple
[alias]
	co = checkout
	cm = commit
	st = status
	br = branch
	dc = diff --cached
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	all-hist = log --all --graph --pretty=format:'%Cred%h%Creset -%C(auto)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit --date=relative
